html{font-size:100%;overflow-x:hidden}body{margin:0;background:#0f1212 no-repeat padding-box;color:#fff;font-family:work sans,sans-serif;overflow-x:hidden;box-sizing:border-box;--main-margin-left:  1rem;--main-margin-right: 1rem;overflow-x:hidden}@media(min-width:1200px){body{--main-margin-left: 2rem;--main-margin-right: 2rem}}@media(min-width:1600px){body{--main-margin-left: 5vw;--main-margin-right: 5vw}}@media(min-width:1920px){body{--main-margin-left:  calc((354 / 1920) * 100vw);--main-margin-right: calc((344 / 1920) * 100vw)}}h1,h2,h3,h4,.h1,.h2,.h3,.h4,.header-template{font-family:space grotesk,sans-serif}.txt-XXL{font-size:3.375rem}.txt-XL{font-size:2.8125rem}.txt-L{font-size:1.375rem}.txt-MD{font-size:16}.f-w300{font-weight:300}.f-w400{font-weight:400}.f-w700{font-weight:700}@media(max-width:450px){.f-w700{font-size:2.5rem}}.f-w900{font-weight:900}.txt-break{display:block}@media(max-width:450px){.txt-break{font-size:2.5rem}}.sc-banner-wide{width:100%}.sc-banner-wide>ul{padding:0;margin:0;list-style:none;width:100%;position:relative;height:calc(100vh - 5rem)}.sc-banner-wide>ul li{position:absolute;inset:0 0 0 0;width:100%;height:100%;opacity:0;transition:opacity .5s ease-in-out}.sc-banner-wide>ul li.active{opacity:1}.sc-banner-wide>ul li .banner-desc{position:absolute;left:50%;transform:translateX(-50%);top:50%}.sc-banner-wide>ul img.banner-img{width:100%;height:100%;object-fit:cover}